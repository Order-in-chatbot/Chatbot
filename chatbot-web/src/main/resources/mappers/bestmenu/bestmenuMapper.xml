<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
          PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
          "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.chatbot.mappers.bestmenu.bestmenuMapper">

	<select id="bestmenufind" resultType="orderVOResultMap">
		select max(hamburger)
		from order_list
		where storeno=#{storeno}
		group by storeno
	</select>
	
	<resultMap type="com.project.chatbot.domain.OrderVO" id="orderVOResultMap">
		<result property="storename" column="storename"/>
	</resultMap>
	

</mapper>