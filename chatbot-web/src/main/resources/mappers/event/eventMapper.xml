<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
          PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
          "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.chatbot.mappers.event.eventMapper">
	<select id="event" resultType="String">
		select eventcontent
		from event
		where storeno=#{storeno}
	</select>
</mapper>